{
  "name": "intex",
  "version": "1.0.0",
  "description": "Intent + Context Framework for OpenAI Function Calling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc src/index.ts",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "openai",
    "chatbot",
    "intent",
    "context",
    "function-calling",
    "typescript",
    "ai",
    "conversation",
    "framework"
  ],
  "author": "Denys Rybalka",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/denisrybalka/intex.git"
  },
  "dependencies": {
    "openai": "^5.6.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "jest": "^30.0.2",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
